def handle_peak_hold(infft,ticks):
    global auto_rst
    global peakhold
    global auto_init
    global frq_ndx

    lndx = frq_ndx

    #
    # Setup inital auto_rst value
    #
    if (auto_init == False):
        auto_rst = 10*ticks
        auto_init = True

    #
    # Resize if necessary
    #
    nfft = list(infft)
    if (len(peakhold[lndx]) &lt; len(infft)):
        peakhold = [nfft]*NCHAN

    #
    # Time for auto peak-hold reset (done on startup as a convenience)
    #
    auto_rst -= 1
    if (auto_rst == 0):
        for n in range(NCHAN):
            peakhold[n] = list(get_raw_fft(n))

    #
    # Do the peak-hold math
    #
    for i in range(0,len(peakhold[lndx])):
        if (infft[i] &gt; peakhold[lndx][i]):
            peakhold[lndx][i] = infft[i]

    return None

